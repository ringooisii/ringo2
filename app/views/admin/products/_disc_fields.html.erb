<div class="nested-fields well well-compact">
  <div class="form-group">
    <%= f.label :No %>
    <%= f.select :disc_number, 1...6 %>
    <%= f.label :ディスク名 %>
    <%= f.text_field :disc_name %>
    <%= link_to_remove_association "ディスク削除", f %>
  </div>

  <div class="songs">
  	<%= f.fields_for :songs do |song| %>
  		<%= render 'song_fields', f: song %>
  	<% end %>
  	<div class="links">
  		<%= link_to_add_association "曲追加", f, :songs %>
  	</div>
  </div>
</div>